{
  "name": "playlist",
  "version": "0.0.1",
  "description": "Music PlayList APP",
  "main": "index.js",
  "scripts": {
    "start": "tsc; npm run copy:static; npm run copy:views; node dist/backend/index.js",
    "webpack:dev": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "webpack:prod": "webpack --config webpack-production.config.js --progress --colors",
    "update": "npm outdated",
    "copy:static": "cp -a src/static dist",
    "copy:views": "mkdir -p dist/backend; cp -a src/backend/views dist/backend",
    "cli": "tsc; node dist/cli/index.js",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tslint": "tslint src/**/*.ts",
    "clean": "rm -rf dist/*; rm -rf node_modules/*",
    "uglify": "uglify -s "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tombatossals/playlist.git"
  },
  "keywords": [
    "node",
    "spotify"
  ],
  "author": "David Rubert",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tombatossals/playlist/issues"
  },
  "homepage": "https://github.com/tombatossals/playlist#readme",
  "dependencies": {
    "axios": "^0.7.0",
    "body-parser": "^1.14.1",
    "bunyan": "^1.5.1",
    "express": "^4.13.3",
    "jade": "^1.11.0",
    "jsonwebtoken": "^5.4.1",
    "mongoose": "^4.2.8",
    "morgan": "^1.6.1",
    "redis": "^2.4.2",
    "spotify-web-api-node": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "babel-plugin-dev-expression": "^0.1.0",
    "babel-runtime": "^5.8.25",
    "codemirror": "^5.9.0",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.3",
    "history": "^1.14.0",
    "html-webpack-plugin": "^1.7.0",
    "intl": "^1.0.1",
    "material-ui": "^0.14.0-rc1",
    "raw-loader": "^0.5.1",
    "react": "^0.14.3",
    "react-addons-linked-state-mixin": "^0.14.0",
    "react-addons-perf": "^0.14.0",
    "react-addons-pure-render-mixin": "^0.14.3",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-motion": "^0.3.1",
    "react-router": "^1.0.0-rc1",
    "react-swipeable-views": "^0.3.0",
    "react-tap-event-plugin": "^0.2.1",
    "transfer-webpack-plugin": "^0.1.4",
    "tslint": "^3.0.0",
    "typescript": "^1.6.2",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1",
    "yargs": "^3.30.0"
  },
  "engine": "node 0.12"
}
